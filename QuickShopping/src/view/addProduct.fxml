<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<Pane maxHeight="400" maxWidth="600" minHeight="400" minWidth="600" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddProductController">
   <children>
      <TextField fx:id="product" layoutX="51.0" layoutY="139.0" prefHeight="31.0" prefWidth="501.0" style="-fx-background-color: #FFFFFF; -fx-border-color: #000000;">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></TextField>
      <ComboBox fx:id="category" layoutX="51.0" layoutY="59.0" prefHeight="31.0" prefWidth="501.0" style="-fx-background-color: #FFFFFF; -fx-border-color: #000000;" />

      <Button fx:id="backBtn" layoutX = "10.0" layoutY="326.0" mnemonicParsing="false" onAction="#back" style="-fx-background-color: #FFFFFF;">
      	<graphic>
      			<ImageView>
      				<image>
      					<Image url="@/pics/back.png" />
      				</image>
      			</ImageView>
		</graphic>
	  </Button>
      <Button fx:id="saveBtn" layoutX="84.0" layoutY="326.0" mnemonicParsing="false" onAction="#addProducy" style="-fx-background-color: #FFFFFF;">
      	<graphic>
			<ImageView>
				<image>
					<Image url="@/pics/save.png" />
				</image>
			</ImageView>
		</graphic>
	  </Button>
      <Label layoutX="51.0" layoutY="14.0" text="Kategoria">
         <font>
            <Font name="Freestyle Script" size="32.0" />
         </font>
      </Label>
      <Label layoutX="51.0" layoutY="94.0" text="Nazwa produktu">
         <font>
            <Font name="Freestyle Script" size="32.0" />
         </font>
      </Label>
   </children>
</Pane>
