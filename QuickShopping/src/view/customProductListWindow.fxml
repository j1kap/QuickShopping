<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<Pane maxHeight="400" maxWidth="600" minHeight="400" minWidth="600" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomProductListController">
   <children>
      <Label fx:id="shop" layoutX="10.0" layoutY="19.0" prefHeight="33.0" prefWidth="580.0" style="-fx-background-color: #FFFFFF;" text="Label">
         <font>
            <Font name="Freestyle Script" size="32.0" />
         </font></Label>
      <TableView fx:id="tabProduct" layoutX="10.0" layoutY="64.0" prefHeight="260.0" prefWidth="580.0">
        <columns>
            <TableColumn fx:id="nameCol" prefWidth="579.0" text="Nazwa" />
        </columns>
      </TableView>
      <Button fx:id="save" layoutX="84.0" layoutY="326.0" mnemonicParsing="false" onAction="#save" style="-fx-background-color: #FFFFFF;">
      	<graphic>
			<ImageView>
				<image>
					<Image url="@/pics/save.png" />
				</image>
			</ImageView>
		</graphic>
	  </Button>
      <Button fx:id="back" layoutX="10.0" layoutY="326.0" mnemonicParsing="false" onAction="#back" style="-fx-background-color: #FFFFFF;">
      	<graphic>
			<ImageView>
				<image>
					<Image url="@/pics/back.png" />
				</image>
			</ImageView>
		</graphic>
	  </Button>
   </children>
</Pane>
