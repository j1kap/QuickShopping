<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<Pane maxHeight="400" maxWidth="600" minHeight="400" minWidth="600" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CaterogryPositionController">
   <children>
      <TableView fx:id="table" layoutX="10.0" layoutY="64.0" prefHeight="260.0" prefWidth="580.0">
        <columns>
          <TableColumn fx:id="position" prefWidth="60.0" resizable="false" text="Pozycja" />
          <TableColumn fx:id="name" prefWidth="519.0" resizable="false" text="Kategoria" />
        </columns>
      </TableView>
      <Button fx:id="btnUp" layoutX="158.0" layoutY="326.0" mnemonicParsing="false" onAction="#upCategory" style="-fx-background-color: #FFFFFF;">
      	<graphic>
      			<ImageView>
      				<image>
      					<Image url="@/pics/up.png" />
      				</image>
      			</ImageView>
		</graphic>
	  </Button>
      <Button fx:id="btnDown" layoutX="84.0" layoutY="326.0" mnemonicParsing="false" onAction="#downCategory" style="-fx-background-color: #FFFFFF;">
      	<graphic>
			<ImageView>
				<image>
					<Image url="@/pics/down.png" />
				</image>
			</ImageView>
		</graphic>
	  </Button>
      <Button fx:id="btnBack" layoutX="10.0" layoutY="326.0" mnemonicParsing="false" onAction="#backBtn" style="-fx-background-color: #FFFFFF;">
      	<graphic>
			<ImageView>
				<image>
					<Image url="@/pics/back.png" />
				</image>
			</ImageView>
		</graphic>
	  </Button>
      <Label fx:id="label" layoutX="10.0" layoutY="18.0" prefHeight="46.0" prefWidth="580.0" style="-fx-background-color: #FFFFFF;" text="Label">
         <font>
            <Font name="Freestyle Script" size="32.0" />
         </font>
         </Label>
   </children>
</Pane>
